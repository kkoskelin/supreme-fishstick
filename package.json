    "lint:css:fix": "stylelint ./src/**/*.css --fix",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "parcel",
    "open": "parcel --open",
    "test": "jest --config jest.config.js --collectCoverage true src/"
